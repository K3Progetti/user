{
    "name": "K3Progetti/user",
    "type": "library",
    "description": "Symfony base user configuration",
    "keywords": [
        "user",
        "symfony",
        "symfony-user"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "K3\\User\\": "src/",
            "Test\\K3\\User\\": "tests/"
        }
    },
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "beberlei/assert": "^3.2",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/orm": "^2.7",
        "K3Progetti/repository": "^1.0",
        "K3Progetti/requestparams": "^2.0",
        "K3Progetti/result": "^1.0",
        "nesbot/carbon": "^2.27",
        "phpspec/prophecy": "^1.10",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/security-bundle": "^5.1",
        "symfony/serializer-pack": "^1.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "sort-packages": true
    },
    "version": "4.1.2"
}
